pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'build successfully'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy successfully'
            }
        }
        stage('test') {
            steps {

                bat label: 'Install npm Package', script: 'npm install'
                bat label: 'Install npm Package', script: 'npm run cy-run'
                echo 'tested successfully'
            }
        }
        stage('release') {
            steps {
                echo 'release successfully'
            }
        }
    }
}