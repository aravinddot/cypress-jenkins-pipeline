pipeline {
    agent any
    tools {nodejs "Node12"}
    stages {
        stage('build') {
            steps {
                echo 'build successfully'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy successfully'
            }
        }
        stage('test') {
            steps {

                sh 'npm install'
                sh 'npm run cy-run'
                echo 'tested successfully'
            }
        }
        stage('release') {
            steps {
                echo 'release successfully'
            }
        }
    }
}